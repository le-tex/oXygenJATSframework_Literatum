<!ENTITY % coreattrs
            "id         ID                     #IMPLIED 
             class      CDATA                  #IMPLIED 
             style      %stylesheet;           #IMPLIED  
             title      %text;                 #IMPLIED"             >


<!--                    INTERNATIONALIZATION ATTRIBUTES            -->
<!--         lang       Language code
             dir        Direction for weak/neutral text            -->
<!ENTITY % i18n
            "lang       %languagecode;         #IMPLIED 
